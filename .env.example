# MongoDB Connection URI
MONGO_URI=mongodb://localhost:27017/payments

# JWT Secret Key
JWT_SECRET=your_jwt_secret_here

# Stripe Configuration
STRIPE_SECRET_KEY=your_stripe_secret_key_here

# Stripe Webhook Secret
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here

# Application Port
PORT=3000

# Stripe Plan Configuration
STRIPE_PLAN_NAME=Premium Subscription
STRIPE_PLAN_AMOUNT=5000
STRIPE_PLAN_CURRENCY=usd
STRIPE_PLAN_INTERVAL=month
STRIPE_PLAN_INTERVAL_COUNT=1
STRIPE_PLAN_DESCRIPTION=Monthly Premium Plan

# Payment Provider
PLAN_PROVIDER=stripe

# Seed Admin User Credentials
SEED_ADMIN_EMAIL=admin@example.com
SEED_ADMIN_PASSWORD=your_admin_password_here
SEED_ADMIN_NAME=Admin User
